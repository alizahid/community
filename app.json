{
  "expo": {
    "name": "Community",
    "slug": "community",
    "scheme": "community",
    "version": "1.0.0",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./src/assets/expo/adaptive-icon.png"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "icon": "./src/assets/expo/icon.png",
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "Community would like to access your photos."
        }
      ]
    ],
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./src/assets/expo/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "automatic"
  }
}
